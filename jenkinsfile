pipeline{
//agent any
    agent any
    tools { 
      maven 'MAVEN_HOME' 
      jdk 'JAVA_HOME' 
    }
    stages{
    	stage('Build jar'){
    		steps{
    		try{
    		    sh "mvn clean test"
    		}catch(error){
    		    retry(3){
    		       sh "mvn clean test" 
    		    }
    		}   			
    	}
    }
 }

}


